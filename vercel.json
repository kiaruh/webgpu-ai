{
    "framework": "nextjs",
    "buildCommand": "cd packages/client && npm install && npm run build",
    "outputDirectory": "packages/client/.next",
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cross-Origin-Opener-Policy",
                    "value": "same-origin"
                },
                {
                    "key": "Cross-Origin-Embedder-Policy",
                    "value": "require-corp"
                }
            ]
        }
    ]
}